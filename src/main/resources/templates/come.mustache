{{> header}}

<div class="mxt-wrapper--header">
    <h1 class="text-center pt-5">{{#i18n}}come.title{{/i18n}}</h1>
    <p class="text-center">
        Manufacture des tabacs - 6 rue Professeur Rollet - 69008 Lyon
    </p>
</div>

<div class="container mxt-wrapper--page-content">
    <p class="text-center lead">{{#i18n}}come.ticket{{/i18n}}</p>

    <h2 class="text-center">{{#i18n}}come.orientation.tocome.title{{/i18n}}</h2>

    <div>
        Conference take place at <a target="_blank" href="http://www.univ-lyon3.fr/campus-de-la-manufacture-des-tabacs-31126.kjsp">Manufacture des tabacs</a> : 6 rue Professeur Rollet
        <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d1222.4017769127167!2d4.861762700872849!3d45.74885156583528!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47f4ea7236bdf0d9%3A0xe6b51aaf8ec933da!2s6+Rue+Professeur+Rollet!5e0!3m2!1sen!2sfr!4v1556227083061!5m2!1sen!2sfr"
                width="100%" height="300" frameborder="0" style="border:0;width:100%" allowfullscreen></iframe>
        <a href="https://www.google.com/maps/place/6+Rue+Professeur+Rollet/@45.7488516,4.8617627,18.19z"
           target="_blank">{{#i18n}}come.map.link{{/i18n}}</a><br/>
    </div>

    <p class="text-center lead">{{#i18n}}come.map.label{{/i18n}}</p>

    <div class="row align-middle">
        <div class="column small-1">
            <img src="/images/svg/transport/primary/transport_flight.svg" class="mxt-icon--social" alt="Come by flight">
        </div>
        <div class="column small-11">
            <p>{{#i18n}}come.plane.detail{{/i18n}}</p>
        </div>
    </div>
    <div class="row align-middle">
        <div class="column small-1">
            <img src="/images/svg/transport/primary/transport_railway.svg" class="mxt-icon--social" alt="Come by train">
        </div>
        <div class="column small-11">
            <p>{{#i18n}}come.train.detail{{/i18n}}</p>
        </div>
    </div>
    <div class="row align-middle">
        <div class="column small-1">
            <img src="/images/svg/transport/primary/transport_bus.svg" class="mxt-icon--social" alt="Come by public transportation">
        </div>
        <div class="column small-11">
            <p>{{#i18n}}come.public.detail{{/i18n}}</p>
        </div>
    </div>
    <div class="row align-middle">
        <div class="column small-1">
            <img src="/images/svg/transport/primary/transport_bike.svg" class="mxt-icon--social" alt="Come by bike">
        </div>
        <div class="column small-11">
            <p>{{#i18n}}come.bike.detail{{/i18n}}</p>
        </div>
    </div>
    <div class="row align-middle">
        <div class="column small-1">
            <img src="/images/svg/transport/primary/transport_car.svg" class="mxt-icon--social" alt="Come by car">
        </div>
        <div class="column small-11">
            <p>{{#i18n}}come.car.detail{{/i18n}}</p>
        </div>
    </div>
</div>

<div class="container mxt-wrapper--page-content">
        <h2 class="text-center">{{#i18n}}come.orientation.manuf.title{{/i18n}}</h2>

        <p class="text-center lead">{{#i18n}}come.orientation.manuf.lead{{/i18n}}</p>


        <p>{{#i18n}}come.orientation.manuf.desc{{/i18n}}</p>

        <h3 class="text-center subheader">{{#i18n}}come.floor0.title{{/i18n}}</h3>
        <p>{{#i18n}}come.floor0.summary{{/i18n}}</p>
        <a href="/images/facilities/manu-map-2022-floor0.jpg" alt="Manufacture des tabacs floor 0" target="map">
        <img src="/images/facilities/manu-map-2022-floor0.jpg" alt="Manufacture des tabacs floor 0" style="max-width: 100%">
        </a>
        <h3 class="text-center subheader">{{#i18n}}come.floorm.title{{/i18n}}</h3>
        <p>{{#i18n}}come.floorm.summary{{/i18n}}</p>
        <a href="/images/facilities/manu-map-2022-floores.jpg" alt="Manufacture des tabacs floor Mezzanine" target="map">
        <img src="/images/facilities/manu-map-2022-floores.jpg" alt="Manufacture des tabacs floor Mezzanine" style="max-width: 100%">
        </a>
        <h3 class="text-center subheader">{{#i18n}}come.floor1.title{{/i18n}}</h3>
        <p>{{#i18n}}come.floor1.summary{{/i18n}}</p>
        <a href="/images/facilities/manu-map-2022-floor1.jpg" alt="Manufacture des tabacs floor 1" target="map">
        <img src="/images/facilities/manu-map-2022-floor1.jpg" alt="Manufacture des tabacs floor 1" style="max-width: 100%">
        </a>
        <p>{{#i18n}}come.floor1.sponsor{{/i18n}}</p>
        <a href="/images/facilities/manu-map-2022-sponsor.jpg" alt="Manufacture des tabacs sponsors" target="map">
        <img src="/images/facilities/manu-map-2022-sponsor.jpg" alt="Manufacture des tabacs sponsors" style="max-width: 100%">
        </a>

        <div class="row align-middle">
            <div class="col-6 col-sm-3">A : CAP GEMINI</div>
            <div class="col-6 col-sm-3">B : NEO9</div>
            <div class="col-6 col-sm-3">C : VISEO</div>
            <div class="col-6 col-sm-3">D : HIWAY</div>
            <div class="col-6 col-sm-3">E : PEAKS</div>
            <div class="col-6 col-sm-3">F : ZENIKA</div>
            <div class="col-6 col-sm-3">G : HACK YOUR JOB</div>
            <div class="col-6 col-sm-3">H : WORLDLINE</div>
            <div class="col-6 col-sm-3">I : SOPRA</div>
            <div class="col-6 col-sm-3">J : ENERGY POOL</div>
            <div class="col-6 col-sm-3">K : DATAIKU</div>
            <div class="col-6 col-sm-3">L : CLEVER CLOUD</div>
            <div class="col-6 col-sm-3">M : CA-TS</div>
            <div class="col-6 col-sm-3">N : CONSERTO</div>
            <div class="col-6 col-sm-3">O : ESKER</div>
            <div class="col-6 col-sm-3">P : MICROSOFT</div>
            <div class="col-12 col-sm-3">ASSO : associations Mixette</div>
        </div>
    </div>
</div>

{{#sponsors}}
    <div class="row">
        <div class="col-12 col-lg-10 offset-lg-1">
            <div class="text-center row justify-content-md-center">
                {{#sponsors-main}}
                    <div class="col-4 text-center col-md-3 col-lg-2">
                        {{> sponsor-detail }}
                    </div>
                {{/sponsors-main}}
            </div>
            <div class="text-center row justify-content-md-center">
                {{#sponsors-others}}
                    <div class="col-3 col-md-2 col-lg-1 text-center">
                        {{> sponsor-detail }}
                    </div>
                {{/sponsors-others}}
            </div>
        </div>
    </div>
{{/sponsors}}

{{> footer}}
